@model IEnumerable<Template.Data.ClientApplicationDocument>

<div class="col-lg-4">
    <div class="box">
        <div class="box-header">
            <h3 class="box-title">Attached Documents</h3>
        </div><!-- /.box-header -->
        <div class="box-body no-padding">
            <table class="table table-striped">
                <tr>
                    <th>ID Number</th>
                    <th>Full Name</th>
                    <th>Doc</th>
                    <th></th>
                </tr>
@foreach (var item in Model)
{
    <tr>
        <td>@Html.DisplayFor(modelItem => item.IDNumber)</td>
        <td>@Html.DisplayFor(modelItem => item.fullname)</td>
        <td>@Html.DisplayFor(modelItem => item.documentName)</td>
        <td>@Html.ActionLink("View", "DisplayFile", new { id = item.documentID })</td>
    </tr>
}
            </table>
        </div><!-- /.box-body -->
    </div><!-- /.box -->
</div>

